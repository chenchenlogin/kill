    </select>

    <!--抢购商品，剩余数量减1  于悦一-->
    <update id="updateSurpus"  parameterType="java.lang.Integer">
        UPDATE activity
        SET surplus = surplus - 1
        WHERE
            activity_id= #{activityId}
            AND surplus > 0
    </update>

